<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>TakeoutServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.TakeoutServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TakeoutServlet</servlet-name>
    <url-pattern>/takeout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ResultServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.InputReaderServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ResultServlet</servlet-name>
    <url-pattern>/inputreader</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CreateTakeoutServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.CreateTakeoutServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CreateTakeoutServlet</servlet-name>
    <url-pattern>/createtakeout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteTakeoutServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.DeleteTakeoutServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DeleteTakeoutServlet</servlet-name>
    <url-pattern>/deletetakeout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SelectTakeoutServlet</servlet-name>
    <servlet-class>com.ironyard.servlet.SelectTakeoutServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SelectTakeoutServlet</servlet-name>
    <url-pattern>/selecttakeout</url-pattern>
  </servlet-mapping>

</web-app>




